

@{
  ViewBag.Title = "Product Inventory";
}

<h2>Product Inventory</h2>

<table id="products">
  <thead>
    <tr>
      <th>Name</th>
      <th>Sale Price</th>
    </tr>
  </thead>
  <tbody>
    <tr data-bind="foreach: products">
      <td data-bind="text: Name"></td>
      <td data-bind="text: SalePrice"></td>
    </tr>
  </tbody>
</table>

<script type="text/javascript">
  $(function () {
    var data = "[{'Name':'Hammer','SalePrice':'11.99'},{'Name:'Nail Pack x100','SalePrice':'0.05'}]";
    var view = ko.mapping.fromJSON(data);

    ko.applyBindings(view);
  })
</script>